    <div class="modal fade" id="DepartmentModal" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Departments</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <div class="row">
                            <label class="col-md-2 control-label">Department Name</label>
                            <div class="col-md-10">
                                <input class="form-control" id="DeptInput" />
                            </div>
                        </div>
                    </div>

                    <div class="row my-5">
                        <div class="col-sm-4">
                            <button class="btn btn-secondary addDept">add Department</button>
                        </div>

                        <div class="col-sm-8">
                            <button class="btn btn-warning ApplyEdit" data-Obj="Department">Edit Department</button>
                            <span><small class="text-danger">pick a department to be edited </small></span>
                        </div>

                    </div>
                    <div class="DepartmentsTbl">
                        <table id="DepartmentsTbl" class="table table-responsive table-hover">
                            <thead>
                                <tr>
                                    <th>Departments</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody id="DepartmentsTbody">
                                @if (Model._departmentDto.Any())
                                {
                                    foreach (var Dept in Model._departmentDto)

                                    {
                                        <tr class="Dept-@Dept.ID">
                                            <td data-Obj="Department" data-id="@Dept.ID" data-Name="@Dept.Name" class="Edit">
                                                @Dept.Name
                                            </td>

                                            <td>
                                                <button class="btn Button-Shared deleteDepartment " data-id="@Dept.ID">delete</button>
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
